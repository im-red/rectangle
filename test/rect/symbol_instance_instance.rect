Rectangle {
    id: root
    width: 800
    height: 800
    border_type: Rectangle.Dot
    Rectangle {
        id: tl
        x: root.width / 2
        y: root.height / 2
        fill_color: "#FF0000"
    }
    Rectangle {
        id: tr
        x: tl.x + tl.width
        fill_color: "#00FF00"
    }
    Rectangle {
        id: bl
        x: tl.x
        y: tl.y + tl.height
        fill_color: "#0000FF"
    }
    Rectangle {
        x: tr.x
        y: bl.y
        border_type: bl.border_type
        fill_color: "#000000"
        border_type: Rectangle.Dash
    }
}