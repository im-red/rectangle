def Rectangle {
    enum LineType {
        Solid,
        Dot
    }
    int x: 0
    int y: 0
    int width: 0
    int height: 0
    string color: "#000000"
    int border.width: 1
    string border.color: "#FFFFFF"
    int border.type: Solid
    list<point> points: {}
    void draw() {
        rect r;
        r.x = x;
        r.y = y;
        r.width = width;
        r.height = height;
        r.fill = color;
        r.stroke_width = border.width;
        r.stroke = border.color;
        r.stroke_dasharray = getDasharray(border.type);
        print(r.stroke_dasharray);
        drawRect(r);
    }
    string getDasharray(int type) {
        if (border.type == Solid) {
            return "";
        } else {
            return "5,5";
        }
    }
}